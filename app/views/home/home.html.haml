= content_for :css_placeholder do
  = stylesheet_link_tag "manifests/home"
= content_for :js_placeholder do
  %script{:type => 'text/javascript', :src => "https://maps.googleapis.com/maps/api/js?key=AIzaSyA9zZ5mWAS3ZkfLL9_tpb_flDGwhEiNHNw&sensor=false"}
  = javascript_include_tag "manifests/home"

.navbar.navbar-default.navbar-fixed-top{role: "navigation"}
  .container-fluid
    .navbar-header
      %button.navbar-toggle{"data-target" => ".navbar-collapse", "data-toggle" => "collapse", type: "button"}
        %span.sr-only Toggle navigation
        %span.icon-bar
        %span.icon-bar
        %span.icon-bar
      %a.navbar-brand{href: "#"} Freon Finder | Recent Craigslist Postings for Freon
    .navbar-collapse.collapse
      %ul.nav.navbar-nav.navbar-right
        %li.listings
          %a{href: "#"} Listings
        %li.map
          %a{href: "#"} Map View

.container
  .freon-finder
    .table-responsive
      %table.table.table-striped
        %thead
        .map-container
          %tr
            %th City
            %th State
            %th Zip
            %th Heading
            %th Time code
            %th Permalink
        %tbody
      .spinner
        .spinner-container.container1
          .circle1
          .circle2
          .circle3
          .circle4
        .spinner-container.container2
          .circle1
          .circle2
          .circle3
          .circle4
        .spinner-container.container3
          .circle1
          .circle2
          .circle3
          .circle4
